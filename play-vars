- name: play level variables
  hosts: local
  connection: local
  vars:
    course: ansible
    duration: 10hrs
    triner is: venky
  tasks:
  - name: print the course information
    ansible.builtin.debug:
    #vars should be closed with double quotes ("msg contednt")
      msg: "Hello {{ course }}, duration: {{ duration }}, triner is { triner }}"